{
  "transform": {
    "^.+\\.(t|j)sx?$": "ts-jest"
  },
  "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
  "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"],
  "testPathIgnorePatterns": ["src/__tests__/test_setting.ts"],
  "collectCoverage": true,
  "collectCoverageFrom": ["./src/**"],
  "coverageReporters": ["text-summary", "clover", "json", "lcov", "text"],
  "coverageThreshold": {
    "global": {
      "lines": 80
    }
  }
}
